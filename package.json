{
  "name": "l20n",
  "description": "A localization library for Firefox OS",
  "version": "4.0.0-pre",
  "homepage": "http://l20n.org",
  "author": "Mozilla <l10n-drivers@mozilla.org>",
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Zibi Braniecki",
      "email": "zbraniecki@mozilla.com"
    },
    {
      "name": "Staś Małolepszy",
      "email": "stas@mozilla.com"
    }
  ],
  "bugs": "https://bugzilla.mozilla.org/enter_bug.cgi?product=Firefox%20OS&component=Gaia::L10n",
  "scripts": {
    "test": "grunt merge-conflict lint test",
    "prepublish": "grunt release"
  },
  "directories": {
    "lib": "./src"
  },
  "main": "./dist/compat/node/l20n.js",
  "browser": "./dist/compat/web/l20n-common.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/l20n/l20n.js.git"
  },
  "keywords": [
    "localization",
    "l10n"
  ],
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.6.5",
    "bridge": "gaia-components/bridge",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "commander": "^2.9",
    "eslint": "^2.3.0",
    "glob": "^7.0.3",
    "grunt": "^0.4.5",
    "grunt-babel": "^6.0.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-jsonlint": "^1.0.7",
    "grunt-karma": "^0.12.1",
    "grunt-merge-conflict": "^0.0.2",
    "grunt-mocha-test": "^0.12.7",
    "grunt-notify": "^0.4.5",
    "grunt-rollup": "^0.6.2",
    "grunt-shell": "^1.2.1",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "load-grunt-tasks": "^3.4.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "prettyjson": "^1.1.3",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.4.0",
    "should": "^8.2.2",
    "string.prototype.endswith": "^0.2.0",
    "string.prototype.startswith": "^0.2.0"
  },
  "engine": {
    "node": ">=4.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "es6": true
    },
    "rules": {
      "valid-typeof": 1,
      "no-console": 1,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-extra-semi": 1,
      "no-func-assign": 2,
      "no-unreachable": 1,
      "no-unexpected-multiline": 2,
      "no-sparse-arrays": 2,
      "block-scoped-var": 1,
      "complexity": [
        1,
        18
      ],
      "dot-location": [
        2,
        "property"
      ],
      "no-implied-eval": 2,
      "no-loop-func": 2,
      "no-magic-numbers": [
        1,
        {
          "ignore": [
            -1,
            0,
            1,
            2
          ]
        }
      ],
      "no-useless-call": 1,
      "no-useless-concat": 1,
      "no-delete-var": 2,
      "no-shadow": 1,
      "no-undef-init": 1,
      "no-unused-vars": 1,
      "consistent-return": 1,
      "curly": [
        2,
        "multi-line"
      ],
      "eqeqeq": 2,
      "no-extend-native": 2,
      "no-extra-bind": 1,
      "no-redeclare": 2,
      "array-bracket-spacing": 1,
      "brace-style": [
        1,
        "1tbs"
      ],
      "no-mixed-spaces-and-tabs": 2,
      "prefer-arrow-callback": 1,
      "prefer-const": 1,
      "prefer-template": 0,
      "prefer-spread": 2,
      "quotes": [
        2,
        "single"
      ],
      "no-var": 1,
      "max-depth": [
        2,
        6
      ],
      "max-len": [
        2,
        80
      ],
      "no-nested-ternary": 2,
      "no-unneeded-ternary": 2,
      "strict": [
        2,
        "global"
      ],
      "indent": [
        1,
        2,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "dependencies": {
    "string.prototype.endswith": "^0.2.0",
    "string.prototype.startswith": "^0.2.0"
  }
}
